{
  "version": 2,
  "name": "wg-hauser",
  "builds": [
    {
      "src": "/server/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/graphql/(.*)",
      "dest": "/server/dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ]
}
